RewriteEngine On
RewriteBase /

# index.html direkt durchlassen
RewriteRule ^index\.html$ - [L]

# Existierende Dateien/Ordner normal ausliefern
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule . - [L]

# Alles andere (z.B. /datenschutz, /impressum, /irgendwas) auf index.html umschreiben
RewriteRule . /index.html [L]
